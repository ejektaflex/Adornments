{
  "schemaVersion": 1,
  "id": "adornments",
  "version": "${version}",

  "name": "Adornments",
  "description": "A Sample Mod for Kambrik",
  "authors": [
    "Ejektaflex",
    "Lemonszz (Original Author)"
  ],
  "contact": {
    "homepage": "None yet",
    "sources": "https://github.com/ejektaflex/Adornments",
    "issues": "https://github.com/ejektaflex/Adornments/issues"
  },

  "license": "MIT",
  "icon": "assets/adornments/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.ejekta.adornments.AdornmentMod"
      }
    ],
    "kambrik": [
      {
        "adapter": "kotlin",
        "value": "io.ejekta.adornments.AdornmentMod"
      }
    ]
  },
  "mixins": [
    "adornments.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "~1.18-0",
    "fabric-language-kotlin": ">=1.6.0"
  },
  "suggests": {
    "flamingo": "*"
  }
}
